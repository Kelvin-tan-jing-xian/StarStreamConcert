
            <h3>Update Feedback</h3>
            <form action="/feedback/update/{{content.uuid}}" method="post" id="content-form"
                enctype="multipart/form-data">
                <div class="row">
                    <div class="col">
                        <div class="form-group">
                            <label class="font-italic" for="Rating">Rating</label>
                            <input type="text" class="form-control typeahead" name="Rating" value="{{content.Rating}}"
                                id="content-title" data-provide="typeahead" autocomplete="off" rows="7" required>
                        </div>
            
                        <div class="form-group mb-4">
                            <label class="font-italic" for="feedbackGiven">feedbackGiven</label>
                            <textarea class="form-control" id="feedbackGiven" name="feedbackGiven" required>{{content.feedbackGiven}}</textarea>
                        </div>
                        {{!-- Put ur textarea reply field here --}}
                    </div>
            
                    
            
                <div class="form-group">
                    <div class="row mb-2">
                        
                        <div class="col-12 col-sm-12 col-md-12 col-lg-6">
                            <label class="font-italic">feedbackType</label>
                            <br>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" value="website" name="feedbackType" checked>
                            <label class="form-check-label">Website service </label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" value="streaming" name="feedbackType">
                            <label class="form-check-label">Streaming service</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" value="others" name="feedbackType">
                            <label class="form-check-label">Others</label>
                        </div>
                       
                    </div>
                </div>
          
            
                <br>
            
                <button id="btnSubmit" type="submit" class="btn btn-primary">Save</button>
                <a id="btnCancel" class="btn btn-primary" href="/feedback/retrieve">Cancel</a>
            </form>
            
